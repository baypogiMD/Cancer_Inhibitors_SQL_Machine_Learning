from torch_geometric.loader import DataLoader
from src.gnn_dataset import InhibitorDataset
from src.gnn_models import GCN
import torch

dataset = InhibitorDataset("../data/processed/inhibitors_cleaned.csv")
loader = DataLoader(dataset, batch_size=32, shuffle=True)

model = GCN()
optimizer = torch.optim.Adam(model.parameters(), lr=0.001)
loss_fn = torch.nn.BCEWithLogitsLoss()

for epoch in range(50):
    for batch in loader:
        optimizer.zero_grad()
        out = model(batch)
        loss = loss_fn(out.squeeze(), batch.y)
        loss.backward()
        optimizer.step()
